{
  "name": "Website-GraduateSchool",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=dev node ./bin/www",
    "test": "mocha && mocha test --require blanket --reporter html-cov > coverage.html"
  },
  "dependencies": {
    "async": "^1.3.0",
    "blanket": "^1.1.7",
    "body-parser": "~1.13.1",
    "chai": "^3.0.0",
    "contentful": "^1.1.1",
    "cookie-parser": "~1.3.5",
    "cucumber": "^0.5.2",
    "debug": "~2.2.0",
    "ejs": "~2.3.2",
    "expect.js": "^0.3.1",
    "express": "~4.13.0",
    "flickr": "^0.2.0",
    "forever-monitor": "^1.5.2",
    "konphyg": "^1.4.0",
    "mocha": "^2.2.5",
    "morgan": "~1.6.1",
    "nock": "^2.7.0",
    "passport-facebook": "^2.0.0",
    "request": "^2.58.0",
    "selenium-webdriver": "^2.46.1",
    "serve-favicon": "~2.3.0",
    "should": "^7.0.2",
    "sinon": "^1.15.4",
    "supertest": "^1.0.1",
    "twitter": "^1.2.5",
    "youtube-api": "^1.0.1"
  },
  "config": {
    "blanket": {
      "pattern": [
        "/router/",
        "/router/routes/",
        "/API/"
      ],
      "data-cover-never": "node_modules",
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  }
}
