<!DOCTYPE html>
<html>
  <head>
    <title> Graduate School USA | <%= title %></title>
    <meta charset="utf-8">
    <% if (seoDescription) { %><meta name="description" content="<%= seoDescription %>"><% } %>
    <% if (seoKeywords) { %><meta name="keywords" content="<%= seoKeywords %>"><% } %>
    <% include ../partials/head %>
    <script type="text/javascript" src="/javascripts/gtog.js"></script>
    <link rel="stylesheet" href="/stylesheets/gtog.css">
  </head>
  <body>
    <% include ../partials/header %>
    <div id="main-container" class="container padding15">
        <div class="row">
          <div class="col-lg-9 col-md-9">
            <% if (content.imageUrl) { %>
              <img src="<%= imageUrl %>" style="width:100%;" />
            <% } %>
            <h1>
              <% if (content.title) { %>
                <%- content.title  %>
              <%  } %>
            </h1>
            <h2>
              <% if (content.subtitle) { %>
                <%- content.subtitle  %>
              <%  } %>
            </h2>
            <% for (var i = 0; i < sections.length; i++) { %>
              <% if (sections[i] && (sections[i].title || sections[i].content || sections[i].snippet)) { %>

                <section>
                  <% if (sections[i].collapse && sections[i].title){ %>
                    <a href="#<%= i %>" class="expand-section accordion-toggle" data-toggle="collapse">
                      <span id="Section<%= i+1 %>" class="glyphicon glyphicon-plus-sign"></span><%= sections[i].title %></a>
                          <article id="<%= i %>" class="collapse nested">
                            <% if (sections[i].snippet) { %>
                              <%- markdown(sections[i].snippet || '') %>
                            <%  } %>
                            <% if (sections[i].content) { %>
                              <%- markdown(sections[i].content || '') %>
                            <%  } %>
                          </article>
                     <% } else if (sections[i].title) { %>
                       <h2><%= sections[i].title %></h2><span id="Section<%= i+1 %>"></span>
                       <article id="<%= i %>" class="genericArticle">
                         <% if (sections[i].snippet) { %>
                           <%- markdown(sections[i].snippet || '') %>
                         <%  } %>
                         <% if (sections[i].content) { %>
                           <%- markdown(sections[i].content || '') %>
                         <%  } %>
                       </article>
                  <%   } else { %>
                       <span id="Section<%= i+1 %>"></span>
                       <article id="<%= i %>" class="genericArticle">
                         <% if (sections[i].snippet) { %>
                           <%- markdown(sections[i].snippet || '') %>
                         <%  } %>
                         <% if (sections[i].content) { %>
                           <%- markdown(sections[i].content || '') %>
                         <%  } %>
                       </article>
                  <%  } %>
                </section>
              <%  } %>
              <% } %>
          </div>
          <% if (content.relatedLinks && content.relatedLinks.length > 0) { %>
          <div class="col-lg-3 col-md-3" id="sidebar">
            <div class="spacer mobile"></div>
            <div class="panel-group">
              <div class="panel panel-default related-links">
            <div class="panel-heading related-links">
                  <h4 class="panel-title">
                    <a data-toggle="collapse" href="#related-links">Related links</a>
                  </h4>
            </div>
            <div id="related-links" class="panel-collapse collapse in">
              <div class="panel-body">
                <ul>
                <% content.relatedLinks.forEach(function(link) { %>
                  <li><a href="<%= link.url %>"><%= link.name %></a></li>
                  <hr>
                <% }); %>
                </ul>
              </div>
            </div>
          </div>
        </div>
        </div>
          <% } %>
        </div> <!-- END row -->
<!-- GTOG STARTS - TO BE REMOVED -->
<hr>
<div class="row">
  <div class="col-lg-12">
    <h1 class="g2g">Guaranteed-to-Go classes by Curriculum</h1>
    <!-- Guaranteed to Go Classes -->
    <!-- Navigation -->
    <div id="gtog">
      <% var keys = Object.keys(curriculumSessions); %>
        <ul class="nav nav-tabs">
          <% for (var i = 0; i < keys.length; i++) { %>
            <% var cssClass = "tab";%>
            <% if (i === 0) { %>
              <% cssClass = "active tab";%>
            <% } %>
            <li class="<%=cssClass%>"><a data-toggle="tab" href="#a<%=i%>"><%=keys[i]%></a></li>
          <% } %>
        </ul>
      <!-- Content -->
      <div id="gtog-content" class="tab-content">
           <% for (var i = 0; i < keys.length; i++) { %>
             <% var className = "tab-pane fade";%>
             <% if (i === 0) { %>
               <% className = "tab-pane fade in active";%>
             <% } %>
             <div id="a<%=i%>" class="<%=className%>">
              <div id="gtog-table"> <!-- start non-mobile view -->
                <table class="table table-striped gtog">
                  <tr>
                    <th>Title</th>
                    <th class="non-mobile">Course Code</th>
                    <th>Start Date</th>
                    <th class="non-mobile">End Date</th>
                    <th>Location</th>
                    <th>Tuition</th>
                    <th>Register</th>
                  </tr>
                  <% var array = curriculumSessions[keys[i]]; %>
                  <% for (var j = 0; j < array.length; j++) { %>
                    <tr>
                      <td><strong><a href="#"><%=array[j].courseTitle%></a></strong></td>
                      <td class="non-mobile"><%=array[j].courseCode%></td>
                      <td><%= array[j].startDate%></td>
                      <td class="non-mobile"><span style="display:none;"><%= array[j].endDate%></span><%= array[j].endDate%></td>
                      <td><%= array[j].location.city%>, <%= array[j].location.state%></td>
                      <td>$<%=array[j].tuition%></td>
                      <td><button class="register" type="button" name="button" onclick="ga('send', 'event', 'register', 'sign-up', '<%=array[j].registrationUrl%>');location.href='<%=array[j].registrationUrl%>'">Register</button></td>
                    </tr>
                  <% } %>
                </table>
              </div> <!--END G2G Classes -->
             </div>
           <% } %>
      </div><!--End of Content -->
    </div><!-- END of Navigation -->
  </div>
</div>  <!-- End of row Guaranteed to Go Classes -->
<!-- GTOG ENDS - TO BE REMOVED -->
</div>

<% include ../partials/chat %>
<% include ../partials/footer %>
</body>
</html>
