<!DOCTYPE html>
<html>
  <head>
    <title> Graduate School USA | <%= title %></title>
    <% include partials/head %>
    <script src="/javascripts/forms.js" type="text/javascript"></script>
</head>
<body>
  <% include partials/header %>
  <!--nutch_noindex-->
  <div class="subscription-type">
    <input type="checkbox" name="txtSubscriptionType" class="subscriptionType" id="subscriptionType1" value="E-mail"
     tabindex="1"> E-mail </input>
  </div>
  <div class="subscription-type">
    <input type="checkbox" name="txtSubscriptionType" class="subscriptionType" id="subscriptionType2" value="Mail"
     tabindex="2"> Mail </input>
  </div>

  <label for="subscription">You would like to</label>
  <input type="radio" name="radSubscription" id="subscribe" value="Subscribe" checked/>
  <label for="subscribe">Subscribe</label>
  <input type="radio" name="radSubscription" id="modify" value="Modify" />
  <label for="modify">Modify</label>
  <input type="radio" name="radSubscription" id="unSubscribe" value="Unsubscribe" />
  <label for="unSubscribe">Unsubscribe</label>

  <fieldset>
    <legend>Please fill out your full name</legend>
    <div class="form-inline">
      <div class="form-group">
        <label for="inputFirstName">First Name:</label>
        <input type="text"  id="txtFirstName" class="form-control" tabindex="1">
        <label for="inputMiddleName">Middle Name:</label>
        <input type="text"  id="txtMiddleName" class="form-control" tabindex="2">
        <label for="inputLastName">Last Name:</label>
        <input type="text"  id="txtLastName" class="form-control" tabindex="3">
      </div>
    </div>
  </fieldset >

  <fieldset>
    <legend>Please fill out your email</legend>
    <label for="inputEmail">Email:</label>
    <input type="text" id="txtEmail">
  </fieldset>

  <fieldset>
    <legend>Please fill out your full name</legend>
    <div class="form-inline">
      <div class="form-group">
        <label for="inputStreet">Street/P.O. Box:</label>
        <input type="text" id="txtStreet" class="form-control" tabindex="1">
        <label for="inputApt">Room/Suite/Apt:</label>
        <input type="text" id="txtApt" class="form-control" tabindex="2">
      </div>
    </div>
    <div class="form-inline">
      <div class="form-group">
        <label for="inputCity">City:</label>
        <input type="text" id="txtCity" class="form-control" tabindex="3">
        <label for="inputState">State:</label>
        <input type="text" id="txtState" class="form-control" tabindex="4">
      </div>
    </div>

  </fieldset>

  <% arrayOfContent.forEach(function(content, num) { %>
    <div class="areaOfInterest-for-subscription">
      <input type="checkbox" name="txtAreaOfInterest" class="areaOfInterest" id="<%= 'areaOfInterest'+num%>" value="<%= content.areaOfInterest %>"
       tabindex="<%= num + 1 %>"> <%= content.areaOfInterest %> </input>
    </div>
  <% }); %>

  <!--/nutch_noindex-->

  <!-- Footer -->
  <% include partials/footer %>
  <!-- END Footer  -->
  <!-- Chat  -->
  <% include partials/chat %>
  <!-- END of Chat  -->

  </body>
</html>
