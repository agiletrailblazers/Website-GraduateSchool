<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
  <% include ../partials/head %>
</head>

<body>
  <% include ../partials/header %>
  <div class="container">
      <div class="col-md-9">
        <p>
          <span class="glyphicon glyphicon-time"></span>
          Posted on <%= date.date('MMM DD, YYYY') %>
        </p>
        <p>
          <%- body %>
        </p>
        <p>
          <% if (featureImageURL !="") { %>
            <img src="<%=featureImageURL %>" alt=""  class=""/>
          <%}%>
        </p>
        <p>
          <a class="btn btn-primary readmore" href='/news'>
            <span class="glyphicon glyphicon-chevron-left"></span>Back to News
          </a>
        </p>
        <br/>
      </div>
    </div>
    <!-- END Container -->
    <!-- END Page Content -->
    <!-- Include Footer Here! -->
    <% include ../partials/footer %>
  </div><!-- END Container-fluid -->
  <!-- Chat  -->
  <% include ../partials/chat %>
    <!-- END of chat  -->
</body>

</html>
