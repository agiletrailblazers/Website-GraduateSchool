<!DOCTYPE html>
<html>
<head>
  <title> Graduate School USA | <%= title %> </title>
  <meta charset="utf-8">
  <script src='https://www.google.com/recaptcha/api.js'></script>
  <% include ../partials/head %>
  <script src="/javascripts/forms/proctor_request_form.js" type="text/javascript"></script>
  <link rel="stylesheet" href="/stylesheets/library/spinners.css">
  <link rel="stylesheet" href="/stylesheets/library/jquery-ui.css">
  <script src="/javascripts/library/jquery-ui.min.js"></script>
</head>
<body>
<% include ../partials/header %>
<div class="container">
  <div class="loading" style="display:none;"></div>
  <div class="row">
    <div class="col-lg-9 col-md-9 col-sm-12">
      <div id="alertError" class="alert alert-danger" role="alert" style="display:none;">
        <span id="removeAlert" class="glyphicon glyphicon-remove pull-right" aria-hidden="true"></span>
      </div>
      <div id="ProctorForm-information">
        <div class="form-inline">
          <div class="form-group">
            <%- sectionHeaderDescription %>
          </div>
        </div>
        <div id="ProctorForm">
          <form>
            <div class="panel-group" id="accordion">
              <div class="panel panel-default">
                <div class="panel-heading gs">
                  <a class="accordion-toggle" id="collapse1Link" data-toggle="collapse" data-parent="#accordion" href="#collapse1">
                    <h4 class="panel-title">STUDENT INFORMATION</h4>
                  </a>
                </div>
                <div id="collapse1" class="panel-collapse collapse in">
                  <div class="panel-body">
                      <div class="form-inline">
                        <div class="form-group">
                          <label for="inputFirstName">*First Name</label>
                          <input id="txtFirstName" name="firstName" type="text" class="form-control" tabindex="1" required>
                          <label for="inputLastName">*Last Name</label>
                          <input id="txtLastName" name="lastName" type="text" class="form-control" tabindex="2"  required>
                        </div>
                      </div>
                      <div class="form-inline">
                        <span class="form-group">
                          <label for="txtStreet">Street Address</label>
                          <input type="text" id="txtStreet" class="form-control" tabindex="3">
                        </span>
                        <span class="form-group">
                          <label for="txtSuite">Suite, Mail Stop</label>
                          <input type="text" id="txtSuite" class="form-control" size="20" tabindex="4">
                        </span>
                      </div>
                      <div class="form-inline">
                        <span class="form-group">
                          <label for="txtCity">City</label>
                          <input type="text" id="txtCity" class="form-control" tabindex="5">
                        </span>
                        <span class=" form-group">
                          <label for="selState">State</label>
                          <select id="selState" class="form-control" tabindex="6">
                            <option value="">Select One</option>
                            <% for (i = 0; i < states.length; i++) { %>
                            <option value="<%= states[i].abbreviation %>"><%= states[i].name %></option>
                            <% } %>
                          </select>
                        </span>
                        <span class="form-group">
                          <label for="txtZip">Zip</label>
                          <input type="text" id="txtZip" class="form-control" tabindex="7">
                        </span>
                      </div>
                      <div class="form-inline">
                        <span class="form-group">
                          <label for="txtPhone">*Daytime Phone Number</label>
                          <input id="telPhone" name="phone" type="tel" placeholder="XXX-XXX-XXXX" pattern="\d{3}[\-]\d{3}[\-]\d{4}" maxlength="12" class="form-control" tabindex="8">
                        </span>
                      </div>
                      <div class="form-inline">
                        <span class="form-group">
                          <label for="txtHomePhone">Home Phone Number</label>
                          <input id="telHomePhone" name="phone" type="tel" placeholder="XXX-XXX-XXXX"  pattern="\d{3}[\-]\d{3}[\-]\d{4}" maxlength="12" class="form-control" tabindex="9">
                        </span>
                      </div>
                      <div class="form-inline">
                        <span class="form-group">
                          <label for="txtEmail">*Email</label>
                          <input type="email" id="txtEmail" class="form-control" size="30" tabindex="10">
                        </span>
                      </div>
                      <div class="form-inline">
                        <span class="form-group">
                          <label for="txtCourseNumber">*Course Code</label>
                          <input type="text" id="txtCourseNumber" class="form-control" size="20" tabindex="11" required>
                        </span>
                        <span class="form-group">
                          <label for="txtCourseTitle">*Course Title</label>
                          <input type="text" id="txtCourseTitle" class="form-control" size="40" tabindex="12" required>
                        </span>
                      </div>
                      <div class="form-inline">
                        <span class="form-group">
                          <label for="txtInstructor">*Instructor Name</label>
                          <input type="text" id="txtInstructor" class="form-control" size="30" tabindex="13" required>
                        </span>
                      </div>
                      <br>
                      <button type="button" name="toPanel2Button" id="toPanel2Button" tabindex="14" class="next-button">Next</button>
                    </div><!-- END Panel-body -->
                  </div><!-- END Collapse1 -->
                </div><!-- END Panel default -->
                <div class="panel panel-default">
                  <div class="panel-heading gs">
                    <a class="accordion-toggle collapsed" id="collapse2Link" data-toggle="collapse" data-parent="#accordion" href="#collapse2">
                      <h4 class="panel-title">PROCTOR INFORMATION</h4>
                    </a>
                  </div>
                  <div id="collapse2" class="panel-collapse collapse">
                  <div class="panel-body">
                    <div class="form-inline">
                        <div class="form-group">
                          <label for="inputProctorFirstName">*First Name</label>
                          <input id="txtProctorFirstName" name="proctorfirstName" type="text" class="form-control" tabindex="15" required>
                          <label for="inputProctorLastName">*Last Name</label>
                          <input id="txtProctorLastName" name="proctorlastName" type="text" class="form-control" tabindex="16" required>
                        </div>
                    </div>
                    <div class="form-inline">
                      <span class="form-group">
                        <label for="inputProctorEmploymentPlace">Place of Employment</label>
                        <input id="txtProctorEmploymentPlace" name="proctoremploymentplace" type="text" class="form-control" tabindex="17" required>
                      </span>
                    </div>
                    <div class="form-inline">
                        <span class="form-group">
                          <label for="inputProctorProfession">Job Title or Profession</label>
                          <input id="txtProctorProfession" name="proctorprofession" type="text" class="form-control" tabindex="18" required>
                        </span>
                    </div>
                    <div class="form-inline">
                        <span class="form-group">
                          <label for="inputProctorStudentRelation">RelationShip to Student</label>
                          <input id="txtProctorStudentRelation" name="proctorstudentrelation" type="text" class="form-control"  tabindex="19" required>
                        </span>
                    </div>
                    <div class="form-inline">
                      <span class="form-group">
                        <label for="txtProctorEmail">*Email</label>
                        <input type="email" id="txtProctorEmail" class="form-control" size="30" tabindex="20">
                      </span>
                    </div>
                    <div class="form-inline">
                      <span class="form-group">
                        <label for="txtProctorPhone">Work Phone Number</label>
                        <input id="telProctorPhone" name="proctorphone" type="tel" placeholder="XXX-XXX-XXXX"  pattern="\d{3}[\-]\d{3}[\-]\d{4}"  maxlength="12" class="form-control" tabindex="21">
                      </span>
                      <span class="form-group">
                        <label for="txtProctorFax">Fax Number</label>
                        <input type="text" id="txtProctorFax" class="form-control" tabindex="22">
                      </span>
                    </div>
                    <br>
                    <button type="button" name="toPanel1Button" id="toPanel1Button" tabindex="23">Previous</button>
                    <button type="button" name="toPanel3Button" id="toPanel3Button" tabindex="24" class="next-button">Next</button>
                    <br>
                  </div>
                </div><!-- END Panel-body -->
              </div><!-- END Collapse2 -->
            </div><!-- END Panel default -->
            <div class="panel panel-default">
              <div class="panel-heading gs">
                <a class="accordion-toggle collapsed" id="collapse3Link" data-toggle="collapse" data-parent="#accordion" href="#collapse3">
                  <h4 class="panel-title">ADDITIONAL MAIL TO</h4>
                </a>
              </div>
              <div id="collapse3" class="panel-collapse collapse">
                <div class="panel-body">
                  <label>Materials for test will be primarily emailed to the proctor.</label>
                  <div class="form-inline">
                   <span class="form-group">
                     <label for="txtProctorEmailTo">Email To</label>
                     <input type="email" id="txtProctorEmailTo" class="form-control" size="30" tabindex="25">
                   </span>
                  </div>
                  <label>Address to mail the examination to if necessary:</label>
                   <div class="form-inline">
                     <span class="form-group">
                       <label for="txtProctorOrganizaiton">Organization Name</label>
                       <input type="text" id="txtProctorOrganizaiton" class="form-control" size="50" tabindex="26">
                     </span>
                   </div>
                   <div class="form-inline">
                     <span class="form-group">
                       <label for="txtProctorAttentionName">Attention Name</label>
                       <input type="text" id="txtProctorAttentionName" class="form-control" size="50" tabindex="27">
                     </span>
                   </div>
                 <div class="form-inline">
                   <span class="form-group">
                     <label for="txtProctorStreet">Street Address</label>
                     <input type="text" id="txtProctorStreet" class="form-control" tabindex="28">
                   </span>
                   <span class="form-group">
                     <label for="txtProctorSuite">Suite, Mail Stop</label>
                     <input type="text" id="txtProctorSuite" class="form-control" size="20" tabindex="29">
                   </span>
                 </div>
                 <div class="form-inline">
                   <span class="form-group">
                     <label for="txtProctorCity">City</label>
                     <input type="text" id="txtProctorCity" class="form-control" tabindex="30">
                   </span>
                   <span class=" form-group">
                     <label for="selProctorState">State</label>
                     <select id="selProctorState" class="form-control" tabindex="31">
                       <option value="">Select One</option>
                       <% for (i = 0; i < states.length; i++) { %>
                       <option value="<%= states[i].abbreviation %>"><%= states[i].name %></option>
                       <% } %>
                     </select>
                   </span>
                   <span class="form-group">
                     <label for="txtProctorZip">Zip</label>
                     <input type="text" id="txtProctorZip" class="form-control" tabindex="32">
                   </span>
                 </div>
                 <br>
                 <div id="captcha">
                   <div class="g-recaptcha" data-sitekey="6Lfj4AsTAAAAAB0NCpQUY3AEeoisB-iNUvzbFsbh"></div>
                 </div>
                 <br>
                 <button type="button" name="toPanel2Button-2" id="toPanel2Button-2" tabindex="33">Previous</button>
                 <button type="submit" id="submitForm" name="submitForm" value="submitForm" tabindex="36">Submit Request</button>
                 <br>
                </div>
              </div><!-- END Collapse3 -->
            </div>
            <%- sectionFooterDescription %>
          </div><!-- End Proctor Request Form -->
      </div> <!-- End Proctor form information -->
      <div id="alertSuccess" class="alert alert-sucess" role="alert" style="display:none;">
        <div><b>Thank you for your interest in Graduate School USA.</b></div>
        <div>Someone will be contacting you as soon as possible.</div>
      </div>
  </div> <!-- END left column -->
    <% if (relatedLinks && relatedLinks.length > 0) { %>
    <div class="col-lg-3 col-md-3 col-sm-12" id="sidebar">
      <div class="panel-group">
        <div class="panel panel-default related-links">
          <div class="panel-heading related-links">
            <h4 class="panel-title">
              <a data-toggle="collapse" href="#related-links">Related links</a>
            </h4>
          </div>
          <div id="related-links" class="panel-collapse collapse in">
            <div class="panel-body">
              <ul>
                <% relatedLinks.forEach(function(link) { %>
                <li><a href="<%= link.url %>"><%= link.name %></a></li>
                <hr>
                <% }); %>
              </ul>
            </div>
          </div>
        </div>
      </div>
  </div> <!-- END right column -->
    <% } %>
  </div> <!-- END Row -->
  <div class="spacer"></div>

</div> <!-- END Container -->

  <% include ../partials/chat %>
  <% include ../partials/footer %>

</body>
</html>
