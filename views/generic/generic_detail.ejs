<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
    <% include ../partials/head %>
    <script type="text/javascript" src="/javascripts/generic_detail.js"></script>
    <link rel="stylesheet" href="/stylesheets/generic_detail.css">
  </head>
  <body>
<!--  Header include -->
    <div class="container-fluid">
      <div class="container">
        <div class="row nm header clearfix">
          <% include ../partials/header %>
        </div>
<!-- /end Row Container -->
      </div>
    </div>
<!-- /end Header include -->
      <div id="main-container" class="container">
        <div class="row">
          <div class="col-lg-8">
            <% for (var i = 0; i < sections.length; i++) { %>
              <% if (sections[i]) { %>
                  <section>
                    <article id="<%= i %>" class="genericArticle">
                      <%- sections[i] %>
                    </article>
                  </section>
              <% } %>
            <%  } %>
          </div>
          <% if (relatedLinks.length > 0) { %>
          <div class="col-lg-4">
            <h2>Related links</h2>
            <ul>
            <% relatedLinks.forEach(function(link) { %>
              <li><a href="<%= link.url %>"><%= link.name %></a></li>
            <% }); %>
            </ul>
          </div>
          <% } %>
        </div>
      </div>

      <% include ../partials/chat %>
      <% include ../partials/footer %>
  </body>
</html>
