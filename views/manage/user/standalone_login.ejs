<!DOCTYPE html>
<html>
  <head>
    <title> Graduate School USA | <%= title %></title>
    <link rel="stylesheet" href="/stylesheets/cart.css" />
    <% include ../../partials/head %>
    <script type="text/javascript" src="/javascripts/manage/user/standalone_login.js"></script>
    <link rel="stylesheet" href="/stylesheets/library/jquery-ui.css">
    <script src="/javascripts/library/jquery-ui.min.js"></script>
  </head>
  <body>
    <% include ../../partials/header %>
      <div id="container" class="container">
        <div id="gs-alert-error" class="alert alert-danger" role="alert" style="display:none;">
          <span id="gs-alert-remove" class="glyphicon glyphicon-remove pull-right" aria-hidden="true"> </span>
          <p></p>
        </div> <!-- END gs-alert-error -->
        <div id="outer" class="col-lg-12 col-md-12 col-sm-12 form-fix">
          <div id="gs-alert-error" class="alert alert-danger" role="alert" style="display:none;">
            <span id="gs-alert-remove" class="glyphicon glyphicon-remove pull-right" aria-hidden="true"> </span>
            <p></p>
          </div> <!-- END gs-alert-error -->

          <% if (loginMessage) { %>
          <div class="center">
            <p><%= loginMessage %></p>
          </div>
          <div class="spacer10"></div>
          <% } %>

          <!-- Login Form Secion -->
          <div id="login-section" class="row">
            <div class="col-lg-12">
                <div id="login">
                  <div id="gs-form">
                    <form id="login-form" role="form">
                      <div id="form-wrap" class="center">
                        <div class="login-stuff">
                          <div class="form-inline">
                            <span class="form-group">
                              <label for="username">Email</label>
                              <input type="email" id="username" name="username" class="form-control" size="50" tabindex="12">
                            </span>
                          </div>
                          <div class="form-inline">
                            <span class="form-group">
                              <label for="credentials">Password</label>
                              <input type="password" id="credentials" name="password" class="form-control" tabindex="13">
                            </span>
                          </div>
                          <div class="form-inline">
                            <span class="form-group">
                              <div id="forgot-password">
                                <a href="/manage/user/password/forgot">Forgot your password?</a>
                              </div>
                            </span>
                          </div>
                          <div class="form-inline">
                            <button id="login-submit" type="button" tabindex="14">Log In</button>
                          </div>
                        </div>
                      </div> <!-- END form-wrap -->
                    </form> <!-- END login-form -->
                  </div>
                </div> <!-- END login -->
            </div>
          </div> <!-- END login-section -->
        </div> <!--END outer-->
      </div> <!--END container-->
    <!-- Footer -->
    <% include ../../partials/footer %>
    <!-- END Footer  -->
    <!-- Chat  -->
    <% include ../../partials/chat %>
    <!-- END of Chat  -->
  </body>
</html>
