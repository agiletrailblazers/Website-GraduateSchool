<div id="reg-loading" class="loading" style="display:none;"></div>
<div id="create-account-section" class="row">
  <h2 class="login-title">Personal Details</h2>
  <div id="gs-form">
    <div id="create-user">
      <form id="create-user-form" role="form">
        <% if (nextpage) { %>
          <input type=hidden id="nextPage" name="nextPage" value="<%= nextpage %>"/>
        <% } %>
        <div id="form-wrap" class="center">
          <div id="personal-info">
            <div class="form-inline">
              <span class="form-group">
                <label for="firstName">*First Name</label>
                <input type="text" id="firstName" class="form-control" tabindex="16">
              </span>
              <span class="form-group">
                <label for="middleName">Middle Name</label>
                <input type="text" id="middleName" class="form-control" tabindex="17">
              </span>
            </div>
            <div class="form-inline">
              <span class="form-group">
                <label for="lastName" >*Last Name</label>
                <input type="text" id="lastName" class="form-control" tabindex="18">
              </span>
               <span class="form-group">
                <label for="email" >*Email</label>
                <input type="email" id="email" class="form-control" size="50" tabindex="19">
              </span>
            </div>
            <div class="form-inline">
              <span class="form-group">
                <label for="password" >*Password</label>
                <input type="password" id="password" class="form-control" tabindex="20">
              </span>
              <span class="form-group">
                <label for="confirmPassword" >*Confirm Password</label>
                <input type="password" id="confirmPassword" class="form-control" tabindex="21">
              </span>
            </div>
            <div class="form-inline">
              <span class="form-group">
                <label for="dateOfBirth" >*Date of birth</label>
                <input type="text" id="dateOfBirth" class="form-control" name="dateOfBirth" placeholder="MM/DD/YYYY" tabindex="22">
              </span>
              <span class="form-group">
                <label for="lastFourSSN" >*Last 4 digits of SSN</label>
                <input type="password" id="lastFourSSN" class="form-control" size="4" maxlength="4" tabindex="23">
              </span>
            </div>
            <hr class="form-divider">
            <div class="form-inline">
              <span class="form-group">
                <label for="street" >*Street Address</label>
                <input type="text" id="street" class="form-control" tabindex="24">
              </span>
              <span class="form-group">
                <label for="suite" >Suite, Mail Stop</label>
                <input type="text" id="suite" class="form-control" tabindex="25">
              </span>
              </div>
            <div class="form-inline">
              <span class="form-group">
                <label for="city" >*City</label>
                <input type="text" id="city" class="form-control" tabindex="26">
              </span>
              <span class="form-group">
                <label for="state" >*State</label>
            <!--nutch_noindex-->
                <select id="state" class="form-control" tabindex="27">
                  <option value="">Select One</option>
                  <% for (i = 0; i < states.length; i++) { %>
                    <option value="<%= states[i].abbreviation %>"><%= states[i].name %></option>
                  <% } %>
                </select>
            <!--/nutch_noindex-->
              </span>
            </div>
            <div class="form-inline">
              <span class="form-group">
                <label for="zip" >*Zip</label>
                <input type="text" id="zip" class="form-control" tabindex="28">
              </span>
              <span class="form-group">
                <label for="phone" >*Daytime Phone Number</label>
                <input type="text" id="phone" class="form-control" tabindex="29">
              </span>
            </div>
            <div class="form-inline">
                <span class="form-group">
                  <label for="timezone" >*Timezone</label>
                    <select id="timezone" class="form-control full-width" tabindex="30">
                      <option value="">Select One</option>
                      <% for (i = 0; i < timezones.length; i++) { %>
                      <option value="<%= timezones[i].id %>"><%= timezones[i].name %></option>
                      <% } %>
                    </select>
                </span>
                <span class="form-group">
                  <div class="form-control" id="field-fixer"></div>
                </span>
              </div>
          </div>
          <div class="spacer10 non-mobile"></div>
          <div id="button-fixer" >
            <div id="buttons" class="pull-right">
              <button id="create-user-form-submit" type="submit" tabindex="31" onclick="window.location.href='#gs-form'">Submit</button>
            </div>
          </div>
        </div> <!-- END form-wrap -->
      </form> <!-- END create-user-form -->
    </div>  <!-- END create-user -->
  </div> <!--END gs-form-->
</div> <!-- END create-account-section -->
