
<!DOCTYPE html>
<html>
<head>
  <title>
    <%= entry.pageTitle %>
  </title>
  <% include partials/head %>
</head>
<body>

  <% include partials/header %>

  <div class="container">
    <div class="row">
      <header>
          <%
          var whatsnewheaderImageURLs= whatsnewheaderImageURLList.split(',');
          for (var i=0; i <whatsnewheaderImageURLs.length;i++) {
              if (whatsnewheaderImageURLs[i] !="") {
          %>
                 <img src="<%= whatsnewheaderImageURLs[i] %>" alt=""  class="img"/>

          <%
               }
          } %>
       </header>
      <div class="col-lg-4 col-md-4 col-sm-12">
      <aside>
          <div class="clearfix">
          <h1 class="box"><%= entry.sidebarmainTitle %></h1>
          <h2 class="title"><%= entry.sidebarTitle %></h2>
          <blockquote cite="">
            <%= entry.sidebarQuote %>
          </blockquote>
          <button type="button" name="button" class="quote pull-right"><%= entry.registerbuttonTitle %></button>
          </div>
        </aside>

      </div>
      <div class="col-lg-8 col-md-8 col-sm-12">
        <section>
          <p>
            <%= entry.sectionSummary %>
          </p>
    <table>
      <tbody>
        <tr>
          <th>
            <strong><%= entry.classNumberTile %></strong>
          </th>
          <th>
            <strong><%= entry.classnameTitle %></strong>
          </th>
        </tr>

            <% for (var i=0; i < entry.sectionClasses.values.length; i++) { %>
                <tr>
                  <td>
                      <a href='/courses/<%=entry.sectionClasses.values[i]["classNumber"] %>'><%=entry.sectionClasses.values[i]["classNumber"].slice(0,-3) %></a>
                  </td>
                  <td>
                      <h5><a href='/courses/<%=entry.sectionClasses.values[i]["classNumber"] %>'><%= entry.sectionClasses.values[i]["className"] %></a></h5>
                  </td>

                </tr>

            <% } %>
          </tbody>
      </table>

        </section>
      </div> <!--/#col -->
    </div> <!--/#row -->
  </div><!--/#container -->
  
  <!-- Footer -->
  <% include partials/footer %>
  <!-- END Footer  -->

  <!-- Chat  -->
  <% include partials/chat %>
  <!-- END of Chat  -->

</body>
</html>
