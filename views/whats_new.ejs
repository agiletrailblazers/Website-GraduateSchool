<!DOCTYPE html>
<html>
<head>
  <title>What's New</title>
  <% include partials/head %>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
  <div class="container">
    <div class="row">
      <% include partials/header %>
      <div class="col-lg-4 col-md-4 col-sm-12">
      <aside>
          <div class="clearfix">
          <h1 class="box">Your success is our mission.</h1>
          <h2 class="title"><%= entry.sidebarTitle %></h2>
          <blockquote cite="">
            <%= entry.sidebarQuote %>
          </blockquote>
          <button type="button" name="button" class="quote pull-right">Register today</button>
          </div>
        </aside>

      </div>
      <div class="col-lg-8 col-md-8 col-sm-12">
        <section>
          <p>
            <%= entry.sectionSummary %>
          </p>
    <table>
      <tbody>
        <tr>
          <th>
            <strong>Course Code</strong>
          </th>
          <th>
            <strong>Course Name</strong>
          </th>
        </tr>

            <% for (var i=0; i < entry.sectionClasses.values.length; i++) { %>
                <tr>
                  <td>
                      <%= entry.sectionClasses.values[i]["classNumber"]%>
                  </td>
                  <td>
                  <h5><%= entry.sectionClasses.values[i]["className"] %></h5>
                  </td>

                </tr>

            <% } %>
          </tbody>
      </table>

        </section>
      </div> <!--/#col -->
    </div> <!--/#row -->
    <% include partials/footer %>
  </div><!--/#container-fulid -->
  <% include partials/chat %>
</body>
</html>
