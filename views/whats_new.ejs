
<!DOCTYPE html>
<html>
<head>
  <title>
    <%= entry.pageTitle %>
  </title>
  <% include partials/head %>
</head>
<body>

  <% include partials/header %>

  <div class="container">
    <div class="row">
      <header>
          <%
          var whatsnewheaderImageURLs= whatsnewheaderImageURLList.split(',');
          for (var i=0; i <whatsnewheaderImageURLs.length;i++) {
              if (whatsnewheaderImageURLs[i] !="") {
          %>
                 <img src="<%= whatsnewheaderImageURLs[i] %>" alt=""  class="img"/>

          <%
               }
          } %>
       </header>

      <div class="col-lg-9 col-md-9 col-sm-12">
        <section>
          <p>
            <%= entry.sectionSummary %>
          </p>
    <table>
      <tbody>
        <tr>
          <th>
            <strong><%= entry.classNumberTile %></strong>
          </th>
          <th>
            <strong><%= entry.classnameTitle %></strong>
          </th>
        </tr>

            <% for (var i=0; i < entry.sectionClasses.values.length; i++) { %>
                <tr>
                  <td>
                      <a href='/courses/<%=entry.sectionClasses.values[i]["classNumber"] %>'><%=entry.sectionClasses.values[i]["classNumber"].slice(0,-3) %></a>
                  </td>
                  <td>
                      <h5><a href='/courses/<%=entry.sectionClasses.values[i]["classNumber"] %>'><%= entry.sectionClasses.values[i]["className"] %></a></h5>
                  </td>

                </tr>

            <% } %>
          </tbody>
      </table>

        </section>
      </div> <!--END of left column -->
      <div class="col-lg-3 col-md-3 col-sm-12">
      <aside>
          <div>
          <h1><%= entry.sidebarmainTitle %></h1>
          <h4><%= entry.sidebarTitle %></h4>
          <blockquote cite="" id="whats-new">
            <span><%= entry.sidebarQuote %></span>
          </blockquote>
          <a href="#" class="blue pull-right"><%= entry.registerbuttonTitle %></a>
          </div>
        </aside>
      </div> <!--END of right column -->
    </div> <!--END row -->
    <div class="spacer"></div>
  </div><!--END container -->

  <!-- Footer -->
  <% include partials/footer %>
  <!-- END Footer  -->

  <!-- Chat  -->
  <% include partials/chat %>
  <!-- END of Chat  -->

</body>
</html>
