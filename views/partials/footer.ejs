<div id="footer" class="container-fluid footer">
  <div class="bar">
    <div id="footer-social-networks-mobile" class="banner mobile">
      <ul class="networks-mobile">
        <li><a id="facebook-link-mobile" href="http://www.facebook.com/GraduateSchoolUSA" target="_blank"><img src="/images/facebook-white.png" alt=""  /></a></li>
        <li><a id="twitter-link-mobile" href="http://twitter.com/thegradschool" target="_blank"><img src="/images/twitter-white.png" alt=""  /></a></li>
        <li><a id="youtube-link-mobile" href="http://www.youtube.com/user/TheGradSchool" target="_blank"><img src="/images/youtube-white.png" alt=""  /></a></li>
        <li><a id="flickr-link-mobile" href="http://www.flickr.com/photos/thegradschool" target="_blank"><img src="/images/flickr-white.png" alt=""  /></a></li>
      </ul>
    </div>
  </div>
  <div class="container">
  <footer>
    <div class="row clearfix non-mobile">
      <div class="col-lg-3 col-md-3 col-sm-3">
        <img id="gs-logo" src="/images/gs-logo-footer.png" alt="" />
        <p id="copyright" class="copyright"> Copyright &copy; 2015 Graduate School USA </p>
        <br>
        <a id="contract-holder-logo" href="/under-construction"><img src="/images/gsa-contract-holder-logo.jpg" alt="GSA Contact Holder" class="center"/></a>
      </div>

      <div id="footer-contact-us" class="col-lg-2 col-md-2 col-sm-3">
        <h3>Contact us</h3>
        <ul class="customer-support footer">
          <li data-placement="left" data-toggle="tooltip" title="Call Graduate School">
            <a id="footer-contact-us-phone" href="tel:888-744-4723" ><img alt="" src="/images/call-us.png" class=""> (888) 744-4723</a>
          </li>
          <li data-placement="left" data-toggle="tooltip" title="Fax Graduate School">
            <a id="footer-contact-us-fax"><img alt="" src="/images/fax-us.png" class=""> (866) 329-4723</a>
          </li>
          <li data-placement="left" data-toggle="tooltip" title="Email Graduate School">
            <a id="footer-contact-us-email" href="/forms/contact-us" ><img alt="" src="/images/email-us.png" class=""> Contact GS</a>
          </li>
          <li data-placement="left" data-toggle="tooltip" title="Chat with Graduate School">
            <a id="footer-contact-us-chat" href="https://livechat.boldchat.com/aid/391752780525725401/bc.chat?resize=true&amp;cbdid=387983771792672006" target="_blank" onclick="window.open((window.pageViewer &amp;&amp; pageViewer.link || function(link){return link;})(this.href + (this.href.indexOf('?')>=0 ? '&amp;' : '?') + 'url=' + escape(document.location.href)), 'Chat391752780525725401', 'toolbar=0,scrollbars=1,location=0,statusbar=0,menubar=0,resizable=1,width=640,height=480');return false;"><img src="/images/chat-with-us.png" alt="" /> Chat</a>
          </li>
        </ul>
      </div>

      <% navigation.footer.forEach(function(nav) { %>
        <div class="col-lg-3 col-md-3 col-sm-3">
          <h3><%= nav.title %></h3>
          <ul class="footer">
            <% if (nav.sections) { %>
              <% if (nav.sections.length == 1) { %>
                <% nav.sections[0].links.forEach(function(link) { %>
                  <li><a href="<%= link.url %>"><%= link.title %></a></li>
                <% }); %>
              <% } else { %>
                <% nav.sections.forEach(function(subNav) { %>
                  <% nav.sections[0].links.forEach(function(link) { %>
                    <li><a href="<%= link.url %>"><%= link.title %></a></li>
                  <% }); %>
                <% }); %>
              <% } %>
            <% } %>
          </ul>
        </div>
      <% }); %>
      <div class="col-lg-3 col-md-3 col-sm-2">
        <div class="center">
          <img id="gs-seal" src="/images/gs-seal.png" alt="Graduate School Seal"  class="seal" />
        </div>
      </div>

      <div id="footer-social-networks" class="col-lg-1 col-md-1 col-sm-1">
        <ul class="networks">
          <li><a id="facebook-link" href="http://www.facebook.com/GraduateSchoolUSA" target="_blank"><img src="/images/facebook.png" alt=""  /></a></li>
          <li><a id="twitter-link" href="http://twitter.com/thegradschool" target="_blank"><img src="/images/twitter.png" alt=""  /></a></li>
          <li><a id="youtube-link" href="http://www.youtube.com/user/TheGradSchool" target="_blank"><img src="/images/youtube.png" alt=""  /></a></li>
          <li><a id="flickr-link" href="http://www.flickr.com/photos/thegradschool" target="_blank"><img src="/images/flickr.png" alt=""  /></a></li>
        </ul>
      </div>
    </div><!-- END non-mobile -->

    <div class="row">
      <div class="spacer"></div>
      <p id="copyright-mobile" class="copyright center mobile"> &copy; 2015 Graduate School USA </p>
      <p id="disclaimer-mobile" class="disclaimer center mobile">
      *Graduate School USA is a private not for profit educational
      institution not affiliated with the Federal Goverment
      or any Federal agency or department.
      </p>
      <div class="spacer mobile"></div>
    </div><!-- END Row  -->
  </footer>
</div><!-- END Container  -->
</div><!-- END Footer and Container fluid -->

