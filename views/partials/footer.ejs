      <% navigation.footer.forEach(function(nav) { %>
        <% if (nav.type=='link') { %>

        <% } else if (nav.type=='sub-section') { %>
          <div class="col-lg-2 col-md-2 col-sm-4">
            <h3><%= nav.name %></h3>
            <ul class="footer">
              <% nav.value.forEach(function(subNav) { %>
                <li><a href="<%= subNav.value %>"><%= subNav.name %></a></li>
              <% }); %>
            </ul>
          </div>
        <% } %>
      <% }); %>
