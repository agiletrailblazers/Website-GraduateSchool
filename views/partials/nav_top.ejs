<div id="search-top-menu">
  <div id="mob-searchBox">
    <form id="search-form" action="javascript:performCourseSearch();">
      <div id="searchGroup" class="input-group">
        <input type="text" id="searchCriteria" name="searchCriteria" class="form-control searchCriteria" placeholder="Search">
        <span class="input-group-btn">
          <button id="course-search-button" class="btn btn-default" type="submit">
            <img alt="" src="/images/search.png">
          </button>
        </span>
      </div>
    </form>
  </div>
  <div id="mob-advancedSearchBox" class="mob-locations">
      <span>Or find a course by:</span>
        <select id="selSearchLocation" class="form-control">
            <option value="">Locations</option>
            <% locations.forEach(function(location) { %>
              <option value="<%= location %>"><%= location %></option>
            <% }); %>
        </select>
  </div>
</div><!-- END Search Top menu -->

<script type="text/javascript">
//Displays search box and advanced filter on focus/mouse enter.
$(document).ready(function(){
  $('#mob-search').click(function() {
      $('#search-top-menu').toggle();
  });
});
$(document).mouseup(function (e)
{
    var container = $("#search-top-menu, #mob-search");

    if (!container.is(e.target) // if the target of the click isn't the container...
        && container.has(e.target).length === 0) // ... nor a descendant of the container
    {
        $('#search-top-menu').fadeOut('medium');
    }
});
</script>
